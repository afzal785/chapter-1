def find_unique_elements(input_list):
    return list(set(input_list))
input_list1 = [3, 7, 3, 5, 2, 5, 9, 2]
print(find_unique_elements(input_list1))
input_list2 = [-1, 2, -1, 3, 2, -2]
print(find_unique_elements(input_list2))
input_list3 = [1000000, 999999, 1000000]
print(find_unique_elements(input_list3)) 
